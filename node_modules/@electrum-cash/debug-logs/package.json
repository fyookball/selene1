{
  "name": "@electrum-cash/debug-logs",
  "version": "1.0.0",
  "description": "Internal component used to create configurable debug logs for @electrum-cash/* packages.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/electrum-cash/debug-logs.git"
  },
  "keywords": [
    "electrum",
    "bitcoin",
    "bitcoin cash"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "source": "./source/index.ts",
  "module": "./dist/index.mjs",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "del ./dist",
    "build": "npm run clean && parcel build",
    "lint": "eslint . --ext .ts",
    "syntax": "tsc --noEmit",
    "spellcheck": "cspell 'source/**' 'test/**' 'examples/**'"
  },
  "dependencies": {
    "debug": "^4.3.7"
  },
  "devDependencies": {
    "@generalprotocols/cspell-dictionary": "git+https://gitlab.com/GeneralProtocols/cspell-dictionary.git",
    "@generalprotocols/eslint-config": "git+https://gitlab.com/GeneralProtocols/eslint-config.git",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "cspell": "^8.3.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.23.4",
    "parcel": "^2.12.0",
    "typescript": "^5.1.3"
  },
  "author": "Jonathan Silverblood",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/electrum-cash/debug-logs/issues"
  },
  "homepage": "https://gitlab.com/electrum-cash/debug-logs#readme"
}
